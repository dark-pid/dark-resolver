[Unit]
Description=dARK Resolver Service
After=network.target

[Service]
User=ubuntu
Group=ubuntu
WorkingDirectory=/home/ubuntu/dark-resolver
#Environment="MANAGED_NAM_DICT={\"8033\":true}"
ExecStartPre=/bin/bash -c 'source /home/ubuntu/dark-resolver/.venv/bin/activate'
ExecStart=/home/ubuntu/dark-resolver/.venv/bin/gunicorn --workers 4 --bind unix:myproject.sock -m 007 server:app

[Install]
WantedBy=multi-user.target